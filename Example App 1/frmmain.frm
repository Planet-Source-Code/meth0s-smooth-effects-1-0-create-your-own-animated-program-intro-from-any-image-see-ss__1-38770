Private Sub tmrmain_Timer()
    picpreview.Visible = False
    picpreview.Cls
    For i = 0 To 492
        If nomore(i) = True Then
            X1(i) = X1(i) - reboundlength
            If X1(i) <= 0 Then
                nomore(i) = False
            End If
        Else
            If X1(i) >= picpreview.Width Then
                X1(i) = X1(i) - reboundlength
                nomore(i) = True
            Else
                X1(i) = X1(i) + reboundlength
            End If
        End If
        If ismore(i) = True Then
            Y1(i) = Y1(i) - reboundlength
            If Y1(i) <= 0 Then
                ismore(i) = False
            End If
        Else
            If Y1(i) >= picpreview.Height Then
                Y1(i) = Y1(i) - reboundlength
                ismore(i) = True
            Else
                Y1(i) = Y1(i) + reboundlength
            End If
        End If
        picpreview.PSet (X1(i), Y1(i)), C1(i)
    Next i
    picpreview.CurrentX = picpreview.Width / 2 - picpreview.TextWidth("Hello!")
    picpreview.CurrentY = picpreview.Height / 2 - picpreview.TextWidth("Hello!")
    picpreview.Print "Hello!"
    picpreview.CurrentY = picpreview.Width / 2 - picpreview.TextWidth("I am a basic intro for a clan or somthing!")
    picpreview.CurrentX = picpreview.Height / 2 + picpreview.TextHeight("I am a basic intro for a clan or somthing!")
    picpreview.Print "I am a basic intro for a clan or somthing!"
    picpreview.Visible = True
End Sub
